{
  "manifest_version": 2,
  "name": "Save me!",
  "version": "1.0",
  "description": "Adds a bookmark.",
  "homepage_url": "https://github.com/erikaszelle/tiaprojekt",

  "icons": {
    "48": "icons/label.png"
  },

  "permissions": [
	  "activeTab",
	  "http://*/"
  ],

  "background": {
    "scripts": ["my-background.js"]
  },

  "browser_action": {
    "default_icon": "icons/label.png",
    "default_title": "Save me!",
	"default_popup": "popup/save_me.html"
  }

}
